{
  "version": "0.0.7",
  "name": "Tests",
  "description": "Runs all unit tests.",
  "startup": {
    "delaySeconds": 1,
    "autoUpdate": true,
    "clearLogs": true
  },
  "deployment": {
    "manifest": [],
    "configScript": [],
    "completeScript": [
      "startup.lua"
    ],
    "completeMessage": "Ready to run unit tests."
  },
  "update": {
    "clean": true
  },
  "logging": {
    "level": "DEBUG",
    "writeToFile": true,
    "maxFileLines": 10000
  },
  "lifetime": {
    "onCrash": "TERMINATE",
    "restartDelaySeconds": 1
  }
}