{
    "version": "0.0.113",
    "name": "GPS",
    "description": "Sets up and runs the GPS.",
    "startup": {
        "delaySeconds": 1,
        "autoUpdate": true,
        "clearLogs": true
    },
    "deployment": {
        "manifest": [
            "/data/config.json"
        ],
        "configScript": [],
        "completeScript": [
            "startup.lua"
        ],
        "completeMessage": "Please note that first time setup will require user action to input the computer coordinates."
    },
    "update": {
        "clean": false
    },
    "logging": {
        "level": "DEBUG",
        "writeToFile": true,
        "maxFileLines": 1000
    },
    "lifetime": {
        "onCrash": "ROLLBACK",
        "restartDelaySeconds": 5
    }
}