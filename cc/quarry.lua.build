	    if prompt ~= nil then
	        Terminal.write(prompt)
	    end
	    return read()
	end
	function Terminal.readNumber(prompt)
	    local value = nil
	    while value == nil do
	        local input = Terminal.read(prompt)
	        value = tonumber(input)
	    end
	    return value
	end
	return Terminal
end
