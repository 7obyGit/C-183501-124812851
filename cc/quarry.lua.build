function MODULE_jobs_definitions_mobile_ProgrammableJob()
	local CodedInventory = utils_storage_codedinventory_CodedInventory
	local ContainerPatternNames = utils_storage_patterns_ContainerPatternNames
	local CommonCodedInventoryListeners = utils_jobutils_CommonCodedInventoryListeners
	local Turtle = turtle_Turtle
	local ProgrammableJob = {}
	ProgrammableJob.__index = ProgrammableJob
	function ProgrammableJob.new(config)
	    local self = setmetatable({}, ProgrammableJob)
	    self:setupCodedInventory()
	    self.backgroundTask = function() self:onBackgroundStep() end
	    self.backgroundDelay = 5  -- This task should be infrequent
	    self.config = config
	    self.states = {
	        COMMAND_PROMPT = "COMMAND_PROMPT",
	        GOING_HOME = "GOING_HOME",
	        MOVING = "MOVING",
	    }
	    return self
	end
	function ProgrammableJob:onStart()
	    self.config:set("alive", true)
	end
	function ProgrammableJob:onResume()
	end
	function ProgrammableJob:inProgress()
	    return self.config:get("alive", false)
	end
	function ProgrammableJob:onStep()
	    self:updateStateUsingCodedInventory()
	    self:runStateAction()
	end
	function ProgrammableJob:onBackgroundStep()
	end
	function ProgrammableJob:onComplete()
	end
	function ProgrammableJob:setupCodedInventory()
	    self.codedInventory = CodedInventory.new()
	    self.codedInventory:addDefaultListeners()
	end
	function ProgrammableJob:updateStateUsingCodedInventory()
	    self.codedInventory:run(self)
	end
	function ProgrammableJob:runStateAction()
	    local state = self.config:get("state", self.states.COMMAND_PROMPT)
	    local actionSide = self.config:get("actionSide", ContainerPatternNames.DIRECTION_FORWARD)
	    if state == self.states.COMMAND_PROMPT then
	        os.sleep(1)
	    elseif state == self.states.MOVING then
	        if actionSide == ContainerPatternNames.DIRECTION_UP then
	            Turtle.up()
	        elseif actionSide == ContainerPatternNames.DIRECTION_DOWN then
	            Turtle.down()
	        else
	            Turtle.forward()
	        end
	    end
	end
	return ProgrammableJob
end
