local jobs_definitions_misc_test_modules_data_ModuleDataTest = MODULE_jobs_definitions_misc_test_modules_data_ModuleDataTest()


function MODULE_jobs_definitions_misc_test_TestJob()
	local Commands = utils_terminal_commands_Commands
	local CommonEventImplementations = utils_jobutils_CommonEventImplementations
	local Job = jobs_Job
	local LogProtocol = utils_network_protocols_logging_LogProtocol
	local ModuleDataTest = jobs_definitions_misc_test_modules_data_ModuleDataTest
	local TestJob = {}
	TestJob.__index = TestJob
	TestJob.name = Job.TEST_JOB
	function TestJob.new(config)
	    local self = setmetatable({}, TestJob)
	    self.modules = {}
	    self.modules.data = ModuleDataTest.new(self)
	    self.backgroundTasks = {}
	    self.commands = Commands.new()
	    self.config = config
	    self.states = {}
	    self.stateActions = {}
	    self.protocols = {
	        log = LogProtocol.new(self),
	    }
	    return self
	end
	function TestJob:onStart()
	    return CommonEventImplementations.onStart(self)
	end
	function TestJob:onResume()
	    return CommonEventImplementations.onResume(self)
	end
	function TestJob:inProgress()
	    return CommonEventImplementations.inProgress(self)
	end
	function TestJob:onStep()
	    print("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n")
	    print("Running tests...")
	    local totalPassed = 0
	    local totalFailed = 0
	    local total = 0
	    for moduleName, module in pairs(self.modules) do
	        local failed = {}
	        local passed = {}
	        for testName, test in pairs(module.__index) do
	            if testName ~= "new" and testName ~= "__index" then
	                local success, result = pcall(test)
	                if not success then
	                    table.insert(failed, {name = testName, result = result})
	                else
	                    table.insert(passed, {name = testName, result = result})
	                end
	            end
	        end
	        local moduleTestCount = #failed + #passed
	        totalPassed = totalPassed + #passed
	        totalFailed = totalFailed + #failed
	        total = total + moduleTestCount
	        if #failed ~= 0 then
	            print(" -> Module: '" .. moduleName .. "' (" .. #passed .. "/" .. moduleTestCount .. ")")
	        end
	        for _, test in ipairs(failed) do
	            print("    '" .. test.name .. "' - " .. test.result)
	        end
	    end
	    print()
	    print("Tests complete: " .. totalPassed .. "/" .. total .. " passed")
	    self.config:set("alive", false)
	end
	function TestJob:onComplete()
	    return CommonEventImplementations.onComplete(self)
	end
	return TestJob
end
